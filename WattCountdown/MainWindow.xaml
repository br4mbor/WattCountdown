<Window x:Class="Abb.Cz.Apps.WattCountdown.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Abb.Cz.Apps.WattCountdown"
        mc:Ignorable="d"
        Title="{Binding Content.Title, ElementName=MainFrame}"
        ResizeMode="CanMinimize" SizeToContent="WidthAndHeight"
        Loaded="Window_Loaded"
        Unloaded="Window_Unloaded">
    <DockPanel>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>
    </DockPanel>
</Window>
